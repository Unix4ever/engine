compositor_node default_node
{
    in 0 renderwindow
    target renderwindow
    {
        pass clear
        {
            colour_value 0.0 0.0 0.0 0.0
        }
        pass render_scene
        {
            rq_first    0
            rq_last     255
        }
        pass custom overlay
        {
        }
    }
}

workspace basic
{
    connect_output default_node 0
}
